  # cppcoreguidelines-avoid-magic-numbers.IgnoredFloatingPointValues: '1.0;100.0;'
  # llvm-else-after-return.WarnOnUnfixable: 'false'
  # bugprone-exception-escape.IgnoredExceptions: ''
